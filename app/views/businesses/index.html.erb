<p id="notice"><%= notice %></p>

<h1>Listing Businesses</h1>

<table>
  <thead>
    <tr>
      <th>Corporate name</th>
      <th>Nickname</th>
      <th>Segment</th>
      <th>Cnpj</th>
      <th>State registration</th>
      <th>Address</th>
      <th>Number</th>
      <th>Complement</th>
      <th>District</th>
      <th>City</th>
      <th>Cep</th>
      <th>Uf</th>
      <th>Country</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>Fax</th>
      <th>Email</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <td><%= business.corporate_name %></td>
        <td><%= business.nickname %></td>
        <td><%= business.segment %></td>
        <td><%= business.cnpj %></td>
        <td><%= business.state_registration %></td>
        <td><%= business.address %></td>
        <td><%= business.number %></td>
        <td><%= business.complement %></td>
        <td><%= business.district %></td>
        <td><%= business.city %></td>
        <td><%= business.cep %></td>
        <td><%= business.uf %></td>
        <td><%= business.country %></td>
        <td><%= business.phone1 %></td>
        <td><%= business.phone2 %></td>
        <td><%= business.fax %></td>
        <td><%= business.email %></td>
        <td><%= business.url %></td>
        <td><%= link_to 'Show', business %></td>
        <td><%= link_to 'Edit', edit_business_path(business) %></td>
        <td><%= link_to 'Destroy', business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business', new_business_path %>
